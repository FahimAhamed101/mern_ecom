{
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@vercel/node" }],
    "rewrites": [{ "source": "/api/authRoutes/(.*)", "destination": "/users" }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
      {
        "src": "/api/productRoutes/(.*)",
        "dest": "/products"
      }
      
    ]
  }